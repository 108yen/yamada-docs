{
  "name": "yamada-docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "prebuild": "pnpm build:contents",
    "build": "contentlayer build && next build",
    "build:profile": "next build --profile",
    "build:changelogs": "tsx scripts/changelog",
    "build:contents": "contentlayer build --clearCache",
    "start": "next start",
    "lint": "next lint",
    "gen:tokens": "yamada-cli tokens theme/index.ts --out node_modules/.pnpm/@yamada-ui+core@0.5.1_@types+react@18.2.15_react-dom@18.2.0_react@18.2.0/node_modules/@yamada-ui/core/dist/generated-theme.types.d.ts",
    "format:check": "prettier --check --cache .",
    "format:write": "prettier --write --cache .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@yamada-ui/calendar": "0.4.7",
    "@yamada-ui/carousel": "0.3.5",
    "@yamada-ui/dropzone": "0.3.7",
    "@yamada-ui/fontawesome": "0.3.5",
    "@yamada-ui/markdown": "0.3.5",
    "@yamada-ui/nextjs": "4.1.5",
    "@yamada-ui/react": "0.7.8",
    "@yamada-ui/table": "0.3.7",
    "contentlayer": "^0.3.4",
    "github-slugger": "^2.0.0",
    "next": "13.5.3",
    "next-contentlayer": "^0.3.4",
    "next-seo": "^6.1.0",
    "prism-react-renderer": "^2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-live": "^4.1.5",
    "remark-breaks": "^4.0.0",
    "remark-emoji": "^4.0.0",
    "remark-gfm": "^3.0.1",
    "remark-slug": "^7.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.2",
    "@commitlint/config-conventional": "17.7.0",
    "@types/mdx": "^2.0.8",
    "@types/node": "20.7.1",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@yamada-ui/cli": "^0.5.0",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.3",
    "lint-staged": "14.0.1",
    "plop": "4.0.0",
    "prettier": "^3.0.3",
    "tsx": "^3.12.10",
    "typescript": "5.2.2",
    "unified": "^10.1.2",
    "unist-util-visit": "^5.0.0"
  }
}

{
  "name": "yamada-docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "contentlayer build && next build",
    "build:profile": "next build --profile",
    "build:changelogs": "tsx scripts/changelog",
    "build:contents": "contentlayer build --clearCache",
    "start": "next start",
    "lint": "next lint",
    "gen:translate": "tsx scripts/translate-content",
    "gen:tokens": "yamada-cli tokens theme/index.ts --out node_modules/.pnpm/@yamada-ui+core@0.5.1_@types+react@18.2.15_react-dom@18.2.0_react@18.2.0/node_modules/@yamada-ui/core/dist/generated-theme.types.d.ts",
    "format:check": "prettier --check --cache .",
    "format:write": "prettier --write --cache .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@yamada-ui/calendar": "0.5.9",
    "@yamada-ui/carousel": "0.3.14",
    "@yamada-ui/dropzone": "0.3.16",
    "@yamada-ui/fontawesome": "0.3.14",
    "@yamada-ui/markdown": "0.3.14",
    "@yamada-ui/nextjs": "4.1.14",
    "@yamada-ui/react": "0.8.11",
    "@yamada-ui/table": "0.4.9",
    "contentlayer": "^0.3.4",
    "github-slugger": "^2.0.0",
    "next": "13.5.6",
    "next-contentlayer": "^0.3.4",
    "next-seo": "^6.1.0",
    "prism-react-renderer": "^2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-live": "^4.1.5",
    "remark-breaks": "^4.0.0",
    "remark-emoji": "^4.0.0",
    "remark-gfm": "^3.0.1",
    "remark-slug": "^7.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@types/mdx": "^2.0.9",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@yamada-ui/cli": "^0.7.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "openai": "^4.12.4",
    "ora": "^7.0.1",
    "plop": "^4.0.0",
    "prettier": "^3.0.3",
    "tsx": "^3.14.0",
    "typescript": "5.2.2",
    "unified": "^10.1.2",
    "unist-util-visit": "^5.0.0"
  }
}

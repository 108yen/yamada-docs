{
  "name": "yamada-docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "prebuild": "pnpm build:content && pnpm build:search",
    "build": "next build",
    "build:profile": "next build --profile",
    "build:changelog": "tsx scripts/changelog",
    "build:theme": "tsx scripts/theme-component",
    "build:props": "tsx scripts/component-props",
    "build:search": "tsx scripts/search-content",
    "build:content": "contentlayer build --clearCache",
    "clean": "rimraf .contentlayer .next",
    "clean:all": "rimraf .contentlayer .next node_modules",
    "start": "next start",
    "lint": "next lint",
    "gen:translate": "tsx scripts/translate-content",
    "gen:tokens": "yamada-cli tokens theme/index.ts",
    "format:check": "prettier --check --cache .",
    "format:write": "prettier --write --cache .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@yamada-ui/calendar": "0.7.1",
    "@yamada-ui/carousel": "0.3.28",
    "@yamada-ui/dropzone": "0.5.10",
    "@yamada-ui/fontawesome": "0.3.23",
    "@yamada-ui/markdown": "0.6.1",
    "@yamada-ui/nextjs": "4.1.23",
    "@yamada-ui/react": "0.9.3",
    "@yamada-ui/table": "0.5.11",
    "contentlayer": "^0.3.4",
    "dayjs": "^1.11.10",
    "glob": "^10.3.10",
    "match-sorter": "^6.3.1",
    "next": "14.0.3",
    "next-seo": "^6.4.0",
    "prism-react-renderer": "^2.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.48.2",
    "react-icons": "^4.12.0",
    "react-live": "^4.1.5",
    "scroll-into-view-if-needed": "^3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@octokit/rest": "^20.0.2",
    "@types/mdx": "^2.0.10",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@yamada-ui/cli": "0.8.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "markdown-toc": "^1.2.0",
    "markdown-wasm": "^1.2.0",
    "next-contentlayer": "^0.3.4",
    "openai": "^4.17.4",
    "ora": "^7.0.1",
    "plop": "^4.0.0",
    "prettier": "^3.0.3",
    "remark-breaks": "^4.0.0",
    "remark-emoji": "4.0.0",
    "remark-gfm": "^3.0.1",
    "remark-slug": "^7.0.1",
    "rimraf": "^5.0.5",
    "tsx": "^4.1.1",
    "typescript": "5.3.2",
    "unified": "^10.1.2",
    "unist-util-visit": "^5.0.0"
  }
}
